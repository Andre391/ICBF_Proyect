@model ICBF_Project.Models.AvanceAcademico

@{
    ViewData["Title"] = "Eliminar AvanceAcademico";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3 class="text-danger">¿Está seguro de eliminar este Avance Academico?</h3>
<hr />
<div class="row">
    <div class="col-sm-4">
        <form asp-action="Eliminar" asp-controller="AvanceAcademico" method="post">
            <input type="hidden" asp-for="PkIdAvanceAcademico" />

            <div class="mb-2">
                <label class="form-label" asp-for="FkNuip">NUIP Niño</label>
                <input class="form-control form-control-sm" asp-for="oNino.PkNuip" type="text" disabled />
            </div>
            <div class="mb-2">
                <label class="form-label" asp-for="Nivel">Nivel</label>
                <input class="form-control form-control-sm" asp-for="Nivel" type="text" disabled />
            </div>
            <div class="mb-2">
                <label class="form-label" asp-for="Notas">Notas</label>
                <input class="form-control form-control-sm" asp-for="Notas" type="text" disabled />
            </div>
            <div class="mb-2">
                <label class="form-label" asp-for="Descripcion">Descripcion</label>
                <input class="form-control form-control-sm" asp-for="Descripcion" type="text" disabled />
            </div>
            <div class="mb-2">
                <label class="form-label" asp-for="FechaEntrega">Fecha Entrega</label>
                <input class="form-control form-control-sm" asp-for="FechaEntrega" type="date" disabled />
            </div>
            <div class="mb-2">
                <button class="btn btn-danger btn-sm w-25" type="submit">Eliminar</button>
                <a class="btn btn-outline-dark btn-sm" asp-action="Index" asp-controller="AvanceAcademico">Volver</a>
            </div>
        </form>
    </div>
</div>

